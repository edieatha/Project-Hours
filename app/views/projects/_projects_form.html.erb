<%= form_with(model: project, local: true, class: "form-group") do |form| %>
  
    <%= form.label :project_name, class: "" %>
    <%= form.text_field :project_name, class: "form-control", placeholder: "project name" %>
 
  <div>
      <%= form.label :project_icon, class: "" %>
      <%= form.select(:project_icon, options_for_select([["📆"], ["📕"], ["📂"], ["🔰"], ["♦️"], ["🏆"], ["🎖"], ["🎯"], ["💼"] ])) %>
  </div>

  <br>

  <div class="actions">
    <%= form.submit class: "btn btn-primary" %>
  </div>
<% end %>
