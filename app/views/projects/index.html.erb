<% provide(:title, "Projects") %>
<% provide(:navbar_page_title, "all my groups") %>

<h1>All projects: <%= @projects.size %> </h1>

<section class="groups">
  <%= render partial: "all_projects", 
  locals: { projects: @projects, allow_change: true} %>
</section>

<br>

  <%= button_to 'Add new project', new_project_path, method: :get, 
  class: 'btn btn-primary add-new-group' %>

  <%= button_to 'back', current_user, method: :get, class: 'btn btn-secondary' %>